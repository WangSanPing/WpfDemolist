<Application x:Class="WpfDemolist.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WpfDemolist"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ResourceDictionary/ButtonControlTemplate.xaml"></ResourceDictionary>
                <ResourceDictionary Source="ResourceDictionary/GradientBrushButton.xaml"></ResourceDictionary>
                <ResourceDictionary Source="ResourceDictionary/CustomListBox.xaml"></ResourceDictionary>
                <ResourceDictionary Source="ResourceDictionary/ListBoxStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
